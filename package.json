{
  "name": "keplertex",
  "displayName": "keplertex",
  "description": "Compile your latex code, no local installations, write your LaTeX code, and compile instantly.",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbeyHurtis/keplertex"
  },
  "icon": "lib/login/logo.png",
  "publisher": "Hurtis",
  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Visualization",
    "Education",
    "Testing"
  ],
  "keywords": [
    "latex",
    "tex",
    "compiler",
    "latex compiler",
    "math",
    "bibtex",
    "BibTeX style",
    "bibtex-style",
    "cpp_embedded_latex",
    "pdf",
    "Cembeddedinlatex",
    "CombinedMarkdownLatex",
    "doctex",
    "doctex-installer",
    "jlweave",
    "latex-workshop-log",
    "latex-class",
    "latex-expl3",
    "LatexPackage",
    "latex-package",
    "latex-workshop",
    "markdown-latex-combined",
    "preview",
    "snippet",
    "weave",
    "weave.jl"
  ],
  "activationEvents": [
    "onLanguage:latex"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keplertex.startRender",
        "title": "Start Inital Rendering"
      }
    ],
    "keybindings": [
      {
        "command": "keplertex.startRender",
        "key": "cmd+k",
        "when": "editorTextFocus"
      }
    ],
    "languages": [
      {
        "id": "latex",
        "extensions": [
          ".tex"
        ],
        "aliases": [
          "LaTeX",
          "latex"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "form-data": "^4.0.3",
    "node-fetch": "^2.7.0"
  }
}
